package org.java.innerClass;

public class InnerClassBasic1Class {
	public static void main(String[] args) {
		InnerClassBaisc1 inner1 = new InnerClassBaisc1();
		inner1.a = 10;
		inner1.instanceMethod();
		
		InnerClassBaisc1.InstanceClass instanceClass = new InnerClassBaisc1().new InstanceClass();
		int b= instanceClass.b=10;
		System.out.println("내부 클래프 b-> " +b);
		instanceClass.m1();
		
		InnerClassBaisc1 inner2 = new InnerClassBaisc1();
		InnerClassBaisc1.InstanceClass2 instanceClass2 = inner2.new InstanceClass2();
		int c=instanceClass2.c=100;
		System.out.println("내부클래스 c->"+c);
}
}

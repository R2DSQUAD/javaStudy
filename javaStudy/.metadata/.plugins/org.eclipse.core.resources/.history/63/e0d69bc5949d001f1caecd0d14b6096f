package org.java.innerClass;

public class InnerClassBaisc1 {
	public int a;
	public static int b;
	
	class InstanceClass{
		int b;
		void m1() {
		}
	}
	
	class InstanceClass2{
		int c;
		void m2() {
			System.out.println("m2()");
		}
	}

	public void instanceMethod() {
		// TODO Auto-generated method stub
		
	}
}
